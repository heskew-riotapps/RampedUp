@{
    Layout = null;
}

<div ng-show="authorized">
    <div class="p-h-md p-v bg-white box-shadow pos-rlt">
        <h3 class="no-margin" translate>Views.Kickstarter.Title</h3>
    </div>
    <div class="p-md">
        <div class="panel panel-default">
            <div class="panel-heading">The basics</div>
            <table st-table="rowCollectionBasic" class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th>first name</th>
                        <th>last name</th>
                        <th>birth date</th>
                        <th>balance</th>
                        <th>email</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="row in rowCollectionBasic" st-select-row="row">
                        <td>{{row.firstName}}</td>
                        <td>{{row.lastName}}</td>
                        <td>{{row.birthDate}}</td>
                        <td>{{row.balance}}</td>
                        <td>{{row.email}}</td>
                    </tr>
                </tbody>
            </table>
        </div>

    </div>
</div>